}

.posts-expand .post-body h2 {
  margin: 45px 0 0.8em;
  padding-bottom: 0.7em;
  border-bottom: 1px solid #ddd;
}
.posts-expand .post-body h3 {
  margin: 52px 0 1.2em;
  line-height: 1.2;
  position: relative;
}
.posts-expand .post-body h3 > a:before {
  content: "#";
  color: #42b983;
  position: absolute;
  left: -0.7em;
  margin-top: -0.05em;
  padding-right: 0.5em;
  font-size: 1.2em;
  line-height: 1;
  font-weight: bold;
}
.posts-expand .post-body figure {
  margin: 1.2em 0;
}
.posts-expand .post-body p {
  line-height: 1.6em;
  margin: 1.2em 0 -1.2em;
  padding-bottom: 1.2em;
  position: relative;
  z-index: 1;
  color: #5e6d82;
}
.posts-expand .post-body ul,
.posts-expand .post-body ol {

  position: inherit;
}
.posts-expand .post-body ul ul,
.posts-expand .post-body ol ul,
.posts-expand .post-body ul ol,
.posts-expand .post-body ol ol {
  margin: 0;
}


