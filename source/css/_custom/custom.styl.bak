}

.posts-expand .post-body h2:before,
.posts-expand .post-body h3:before {
  content: "";
  display: block;
  margin-top: -91px;
  height: 91px;
  visibility: hidden;
}

.posts-expand .post-body h1:before

.posts-expand .post-body h2:before
.posts-expand .post-body h3:before
.posts-expand .post-body h4:before
.posts-expand .post-body h5:before
